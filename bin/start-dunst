#!/usr/bin/env bash

source "/home/patrik/.cache/wal/colors.sh"

pkill dunst
echo $color0

dunst \
        -lb "${color0:-#F0F0F0}" \
        -nb "${color0:-#F0F0F0}" \
        -cb "${color0:-#F0F0F0}" \
        -lf "${color15:=#000000}" \
        -bf "${color15:=#000000}" \
        -cf "${color15:=#000000}" \
        -nf "${color15:=#000000}" &
